{
  "name": "brease-next",
  "version": "1.0.0",
  "description": "Brease CMS integration for Next.js - React components, hooks, and utilities for seamless content management",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist/",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "package",
    "brease",
    "cms",
    "react",
    "headless-cms"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.8.0",
    "@types/react": "^19",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
